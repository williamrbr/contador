algoritmo "contador"
var
      contador, valor, salto:inteiro
inicio
      escreval("Digite um número que você deseja que eu conte:")
      leia(valor)
      escreva("Digite valor do salto: ")
      leia(salto)
		contador <- 0
		enquanto (contador <= valor) faca
		         escreva(contador)
			contador <- contador + salto
fimenquanto
		

fimalgoritmo
